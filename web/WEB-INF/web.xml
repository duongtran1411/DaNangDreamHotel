<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomeController</servlet-name>
        <servlet-class>Controller.HomeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomDetailsController</servlet-name>
        <servlet-class>Controller.RoomDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomController</servlet-name>
        <servlet-class>Controller.ViewRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PagingController</servlet-name>
        <servlet-class>Controller.PagingController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>Controller.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SortRoomController</servlet-name>
        <servlet-class>Controller.SortRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoomOfTypeController</servlet-name>
        <servlet-class>Controller.RoomOfTypeController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EventController</servlet-name>
        <servlet-class>Controller.EventController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingController</servlet-name>
        <servlet-class>Controller.BookingController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/loginControllerURL</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeController</servlet-name>
        <url-pattern>/homeController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomDetailsController</servlet-name>
        <url-pattern>/roomDetailsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomController</servlet-name>
        <url-pattern>/roomController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PagingController</servlet-name>
        <url-pattern>/pagingController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/searchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SortRoomController</servlet-name>
        <url-pattern>/sortRoomController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoomOfTypeController</servlet-name>
        <url-pattern>/roomOfTypeController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EventController</servlet-name>
        <url-pattern>/eventController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingController</servlet-name>
        <url-pattern>/bookingController</url-pattern>
    </servlet-mapping>
</web-app>
